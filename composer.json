{
    "name": "talav/user",
    "description": "Basic user interfaces for PHP applications.",
    "keywords": ["symfony", "bundle", "user", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Talav",
            "email": "support@talav.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "talav/resource": "@dev",
        "symfony/security-guard": "^5.4",
        "symfony/security-http": "^6.0",
        "symfony/security-csrf": "^6.0",
        "symfony/security-core": "^6.0",
        "symfony/doctrine-bridge": "^6.0",
        "symfony/messenger": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/thanks": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Talav\\Component\\User\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Talav\\Component\\User\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../../*/*"
        }
    ]
}
